{
  "name": "suda-gs-ams",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "cross-env VITE_USE_MOCK=true vite",
    "dev:real": "cross-env VITE_USE_MOCK=false vite",
    "build": "tsc -b && vite build",
    "build:preview": "tsc -b && vite build --mode preview",
    "sentry:release": "node ./scripts/sentry-release.mjs",
    "build:with-sentry": "npm run build && npm run sentry:release",
    "build:with-sentry:preview": "npm run build:preview && npm run sentry:release",
    "build:sentry": "npm run build:with-sentry",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@sentry/react": "^10.39.0",
    "antd": "^6.2.2",
    "axios": "^1.13.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.13.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@sentry/cli": "^3.2.2",
    "@types/node": "^25.2.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
